<script>

	import { inView } from "motion";

	const fadeinstatistik = document.querySelector("#who");

	inView(fadeinstatistik, () => {
		fadeinstatistik.classList.add("animate__animated", "animate__fadeInLeft", "animate__delay-0.5s");
	});

	const statistik = document.querySelector("#whatdoyouget");

	inView(statistik, () => {
		statistik.classList.add("animate__animated", "animate__fadeIn", "animate__delay-0.7s");
	});

	const facts = document.querySelector("#whatyougive");

	inView(facts, () => {
		facts.classList.add("animate__animated", "animate__fadeIn", "animate__delay-0.7s");
	});

	const save = document.querySelector("#whyme");

	inView(save, () => {
		save.classList.add("animate__animated", "animate__fadeIn", "animate__delay-0.7s");
	});

	const skills = document.querySelector(".skills_fill");

	document.addEventListener("DOMContentLoaded", function() {
    const skillsContainer = document.getElementById("skills-container");
    const firstBar = document.getElementById("first-bar");
    const secondBar = document.getElementById("second_bar");

    // Create an intersection observer
    const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                // Start the animation here
                firstBar.classList.add("animate"); // Add your animation class
                secondBar.classList.add("animate"); // Add your animation class
                observer.disconnect(); // Stop observing after the animation starts
            }
        });
    }, { threshold: 0.1 }); // Trigger when 10% of the section is visible

    observer.observe(skillsContainer);
});

</script>

<script>
  let slideIndex = 1;
  showSlides(slideIndex);

  function plusSlides(n) {
    showSlides(slideIndex += n);
  }

  function currentSlide(n) {
    showSlides(slideIndex = n);
  }

  function showSlides(n) {
    const slides = document.getElementsByClassName("mySlides");
    const dots = document.getElementsByClassName("dot");

    if (n > slides.length) slideIndex = 1;
    if (n < 1) slideIndex = slides.length;

    for (let i = 0; i < slides.length; i++) {
      slides[i].classList.add("hidden");
    }

    for (let i = 0; i < dots.length; i++) {
      dots[i].classList.remove("bg-gray-700");
      dots[i].classList.add("bg-gray-400");
    }

    slides[slideIndex - 1].classList.remove("hidden");
    dots[slideIndex - 1].classList.remove("bg-gray-400");
    dots[slideIndex - 1].classList.add("bg-gray-700");
  }
</script>